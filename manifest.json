{

    "manifest_version": 2,
    "name": "Mendhak's Flickr",
    "version": "1.3",
  
    "description": "Replaces Firefox New Tab, with photos from Mendhak's Flickr stream",
  
    "icons": {
      "48": "icons/favicon.png"
    },

    "chrome_url_overrides": {
        "newtab":"flickrtab.html"
    },
    "homepage_url": "https://github.com/mendhak/firefox-mendhak-flickr",
    
    "permissions": []
     
  
  }