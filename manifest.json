{

    "manifest_version": 2,
    "name": "Flickr Photos In New Tab",
    "version": "1.4",
  
    "description": "Replaces Firefox New Tab, with photos from a Flickr album of your choice",
  
    "icons": {
      "48": "icons/flickr-tab-frog-3.png"
    },

    "chrome_url_overrides": {
        "newtab":"flickrtab.html"
    },
    "homepage_url": "https://github.com/mendhak/firefox-mendhak-flickr",
    
    "permissions": ["storage"],
     
    "options_ui": {
      "page": "options.html"
    },
  
 
    "browser_specific_settings": {
      "gecko": {
        "id": "{0d92d791-1db2-4181-a017-f309b99a620c}"
      }
    }
  
  }